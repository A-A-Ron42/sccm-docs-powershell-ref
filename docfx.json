{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md"
        ],
        "src": "docs-conceptual",
        "version": "sccm-ps",
        "dest": "sccm",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "toc.yml"
        ],
        "src": "docs-conceptual",
        "version": "sccm-ps",
        "dest": "sccm/sccm"
      },
      {
        "files": [
          "**/*.md"
        ],
        "src": "sccm-ps",
        "version": "sccm-ps",
        "dest": "module"
      },
      {
        "files": [
          "toc.yml"
        ],
        "src": "sccm-ps",
        "version": "sccm-ps",
        "dest": "module/sccm-ps"
      },
      {
        "files": [
          "**/*.md"
        ],
        "src": "virtual-directory",
        "dest": "sccm"
      },
      {
        "files": [
          "toc.yml"
        ],
        "src": "bread",
        "version": "sccm-ps",
        "dest": "sccm/bread"
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "versions": {
      "sccm-ps": {
        "dest": "sccm-ps"
      }
    },
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "apiPlatform": "powershell",
      "breadcrumb_path": "/powershell/sccm/bread/toc.json",
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/sccm-docs-powershell-ref",
      "feedback_product_url": "https://configurationmanager.uservoice.com"
    },
    "fileMetadata": {
      "author": { "**/**.md": "aczechowski"},
      "manager": { "**/**.md": "dougeby"},
      "ms.author": { "**/**.md": "aaroncz"},
      "ms.prod": { "**/**.md": "configuration-manager"},
      "ms.technology": { "**/**.md": "configmgr-other"},
      "ms.topic": {
        "**/sccm-ps/ConfigurationManager/**.md": "reference",
        "**/docs-conceptual/**.md": "conceptual"
      },
      "titleSuffix": { "**/**.md": "Configuration Manager"}
    },
    "template": [],
    "dest": "sccm-powershell",
    "markdownEngineName": "markdig"
  }
}
